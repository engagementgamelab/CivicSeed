
.monitor
	.row
		.span12
			h1 Monitor Panel
	.row	
		each instance, i in instances
			.span4
				h3 Game: #{instance}
				p.adminLink#players(data-instance='#{instance}') Players
				p.adminLink#chat(data-instance='#{instance}') Chat Logs
				p.adminLink#questions(data-instance='#{instance}') Questions
				p.adminLink#addPlayer(data-instance='#{instance}') Add Player
				
	.row
		.span12.output

